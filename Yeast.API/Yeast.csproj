<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.4</TargetFramework>
    <Authors>Alexandre Daubricourt</Authors>
    <Company>Quasar</Company>
    <Description>A port of the original JavaScript Yeast (https://github.com/unshiftio/yeast) by Unshiftio.</Description>
    <PackageLicenseUrl>https://github.com/Akronae/Yeast</PackageLicenseUrl>
    <PackageProjectUrl>https://github.com/Akronae/Yeast</PackageProjectUrl>
    <RepositoryUrl>https://github.com/Akronae/Yeast</RepositoryUrl>
    <RepositoryType>GitHub</RepositoryType>
    <PackageTags>Yeast, Generation, Number, Security</PackageTags>
    <PackageReleaseNotes>Fully released.</PackageReleaseNotes>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
  </PropertyGroup>

  <PropertyGroup Condition="'$(Configuration)|$(Platform)'=='Release|AnyCPU'">
    <DocumentationFile>C:\prog\c#\Yeast\Bin\netstandard1.4\Yeast.xml</DocumentationFile>
    <OutputPath>C:\prog\c#\Yeast\Bin\netstandard1.4\</OutputPath>
  </PropertyGroup>

</Project>